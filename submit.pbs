#!/bin/bash
#PBS -l nodes=1:ppn=12:dc2,walltime=0:45:00
#PBS -N wmaSeg
#PBS -V

[ $PBS_O_WORKDIR ] && cd $PBS_O_WORKDIR

module load matlab
module load spm
module load freesurfer

rm -rf tracts

export MATLABPATH=$MATLABPATH:$SERVICE_DIR
matlab -nodisplay -nosplash -r main
if [ -s output.mat ];
then
	echo 0 > finished
else
	echo "output missing"
	echo 1 > finished
	exit 1
fi
